   ],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Define the color palette for the product categories\n",
    "colors = {'Top': '#d9d9d9', 'Set': '#969696', 'kurta': '#bdbdbd', 'Western Dress': '#F0AB00'}\n",
    "\n",
    "# Group the data by state and calculate the total sales\n",
    "sales_by_state = sales_df.groupby('state', group_keys=True)['order_amount_(â‚¹)'].sum()\n",
    "\n",
    "# Determine the number of states and quartiles\n",
    "n_states = len(sales_by_state)\n",
    "quartiles = pd.qcut(sales_by_state, 4, labels=['Q1', 'Q2', 'Q3', 'Q4'])\n",
    "\n",
    "# Get the top 5 states by sales for each quartile\n",
    "top_states = []\n",
    "for quartile in ['Q1', 'Q2', 'Q3', 'Q4']:\n",
    "    top_states += sales_by_state[quartiles == quartile].nlargest(5).index.tolist()\n",
    "\n",
    "# Filter the dataframe to include only the top states\n",
    "filtered_sales = sales_df[sales_df['state'].isin(top_states)]\n",
    "\n",
    "# Group the data by state and product category, and calculate the total sales\n",
    "sales_by_state_product = filtered_sales.groupby(['state', 'product_category'], group_keys=True)['order_amount_(â‚¹)'].sum().reset_index()\n",
    "\n",
    "# Get the top 3 products for each state in each quartile\n",
    "top_products = []\n",
    "for quartile in ['Q1', 'Q2', 'Q3', 'Q4']:\n",
    "    top_products += sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == quartile].index)].groupby('state', group_keys=True).apply(lambda x: x.nlargest(3, 'order_amount_(â‚¹)'))['product_category'].tolist()\n",
    "\n",
    "# Create a figure with four subplots\n",
    "fig, axs = plt.subplots(2, 2, figsize=(16, 12), sharey=False)\n",
    "\n",
    "# Function to create a bar plot for each quartile\n",
    "def create_barplot(quartile_sales, ax, title):\n",
    "    sns.barplot(x='state', y='order_amount_(â‚¹)', hue='product_category', data=quartile_sales, palette=colors, ax=ax)\n",
    "    ax.legend().set_visible(False)\n",
    "    ax.set_title(title)\n",
    "    ax.set_xlabel('State')\n",
    "    ax.set_ylabel('Total Sales (â‚¹)')\n",
    "\n",
    "# Plot sales for quartile 1\n",
    "q1_sales = sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == 'Q1'].index)]\n",
    "q1_sales = q1_sales[q1_sales['product_category'].isin(top_products)]\n",
    "create_barplot(q1_sales, axs[0, 0], 'Sales by Q1 States (lowest revenue quartile)')\n",
    "\n",
    "# Plot sales for quartile 2\n",
    "q2_sales = sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == 'Q2'].index)]\n",
    "q2_sales = q2_sales[q2_sales['product_category'].isin(top_products)]\n",
    "create_barplot(q2_sales, axs[0, 1], 'Sales by Q2 States (second-lowest revenue quartile)')\n",
    "\n",
    "# Plot sales for quartile 3\n",
    "q3_sales = sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == 'Q3'].index)]\n",
    "q3_sales = q3_sales[q3_sales['product_category'].isin(top_products)]\n",
    "create_barplot(q3_sales, axs[1, 0], 'Sales by Q3 States (second-highest revenue quartile)')\n",
    "\n",
    "# Plot sales for quartile 4\n",
    "q4_sales = sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == 'Q4'].index)]\n",
    "q4_sales = q4_sales[q4_sales['product_category'].isin(top_products)]\n",
    "create_barplot(q4_sales, axs[1, 1], 'Sales by Q4 States (highest revenue quartile)')\n",
    "\n",
    "# Adjust the spacing between subplots\n",
    "plt.subplots_adjust(hspace=0.9, wspace=0.21, bottom=0.2)\n",
    "\n",
    "# Rotate the x-axis tick labels for all subplots\n",
    "for ax in axs.flat:\n",
    "    plt.setp(ax.get_xticklabels(), rotation=45, ha='right')\n",
    "\n",
    "# Add a single legend to the figure\n",
    "handles, labels = axs[1, 1].get_legend_handles_labels()\n",
    "fig.legend(handles, labels, loc='center', ncol=len(labels), title='Product Category', fontsize=15, title_fontsize=15)\n",
    "\n",
    "# Set the main title for the figure\n",
    "plt.suptitle(\"Identifying Top 4 Products for Top 5 States in Each Revenue Quartile\", fontsize=25)\n",
    "\n",
    "# Show the figure\n",
    "plt.show()\n",
    "\n",
    "# Save the figure\n",
    "fig.savefig('Top_Products_Top_States.png', dpi=300)\n"
   ]
