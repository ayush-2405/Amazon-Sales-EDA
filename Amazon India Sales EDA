    "# Calculate total revenue by month\n",
    "monthly_revenue = sales_df.groupby('month')['order_amount_(â‚¹)'].sum()\n",
    "\n",
    "# Calculate percentage decrease in revenue from April to May and from May to June\n",
    "percent_decrease_apr_to_may = (monthly_revenue['april'] - monthly_revenue['may']) / monthly_revenue['april'] * 100\n",
    "percent_decrease_may_to_jun = (monthly_revenue['may'] - monthly_revenue['june']) / monthly_revenue['may'] * 100\n",
    "\n",
    "# Calculate total percentage decrease in revenue from April to June\n",
    "total_percent_decrease_q2 = (monthly_revenue['april'] - monthly_revenue['june']) / monthly_revenue['april'] * 100\n",
    "\n",
    "# Print revenue details and percentage decrease for Q2\n",
    "print(f\"Total revenue for April 2022: â‚¹{monthly_revenue['april']:,.2f}\")\n",
    "print(f\"Total revenue for May 2022: â‚¹{monthly_revenue['may']:,.2f}, which is a {percent_decrease_apr_to_may:.2f}% decrease from April.\")\n",
    "print(f\"Total revenue for June 2022: â‚¹{monthly_revenue['june']:,.2f}, which is a {percent_decrease_may_to_jun:.2f}% decrease from May.\")\n",
    "print(f\"Total revenue for Q2 2022 decreased by {total_percent_decrease_q2:.2f}%\")\n",
    "print(\"\\n\")\n",
    "\n",
    "# Calculate and print total revenue by product category\n",
    "revenue_by_category = sales_df.groupby('product_category')['order_amount_(â‚¹)'].sum().sort_values(ascending=False)\n",
    "print(\"Total revenue by product category:\")\n",
    "print(revenue_by_category.apply(lambda x: f\"â‚¹{x:,.2f}\"))\n",
    "print(\"\\n\")\n",
    "\n",
    "# Calculate and print percentage of revenue by product category\n",
    "percent_revenue_by_category = (revenue_by_category / revenue_by_category.sum() * 100).sort_values(ascending=False)\n",
    "percent_revenue_by_category = percent_revenue_by_category.apply(lambda x: f\"{x:.2f}%\")\n",
    "print(\"Percentage of revenue by product category:\")\n",
    "print(percent_revenue_by_category)\n",
    "print(\"\\n\")\n",
    "\n",
    "# Calculate and print average order amount by product category\n",
    "average_order_amount_by_category = sales_df.groupby('product_category')['order_amount_(â‚¹)'].mean().sort_values(ascending=False)\n",
    "print(\"Top 5 product categories by average order amount:\")\n",
    "print(average_order_amount_by_category.head(5))\n",
    "print(\"\\n\")\n",
    "\n",
    "# Calculate and print statistics on cancelled and returned orders\n",
    "cancelled_orders = sales_df[sales_df['ship_status'].isin(['Cancelled', 'Shipped - Lost in Transit'])]\n",
    "returned_orders = sales_df[sales_df['ship_status'].isin(['Shipped - Returned to Seller', 'Shipped - Returning to Seller', 'Shipped - Rejected by Buyer', 'Shipped - Damaged'])]\n",
    "\n",
    "total_cancelled_orders = len(cancelled_orders)\n",
    "total_returned_orders = len(returned_orders)\n",
    "total_cancelled_and_returned_orders = total_cancelled_orders + total_returned_orders\n",
    "\n",
    "percent_cancelled_orders = total_cancelled_orders / len(sales_df) * 100\n",
    "percent_returned_orders = total_returned_orders / len(sales_df) * 100\n",
    "percent_cancelled_and_returned_orders = total_cancelled_and_returned_orders / sales_df['order_quantity'].sum() * 100\n",
    "\n",
    "print(f\"Total cancelled orders: {total_cancelled_orders}, which is {percent_cancelled_orders:.2f}% of all orders.\")\n",
    "print(f\"Total returned orders: {total_returned_orders}, which is {percent_returned_orders:.2f}% of all orders.\")\n",
    "print(f\"This represents {percent_cancelled_and_returned_orders:.2f}% of all orders.\")\n",
    "print(\"\\n\")\n",
    "\n",
    "# Calculate and print average order amount and quantity by month\n",
    "monthly_order_stats = sales_df.groupby(pd.Grouper(key='date', freq='ME')).agg({\n",
    "    'order_amount_(â‚¹)': 'mean', \n",
    "    'order_quantity': 'mean'\n",
    "}).rename(columns={'order_amount_(â‚¹)': 'average_order_amount', 'order_quantity': 'average_order_quantity'})\n",
    "print(monthly_order_stats)\n",
    "print(\"\\n\")\n",
    "\n",
    "# Calculate and print the most popular product category in each state\n",
    "most_popular_category_by_state = sales_df.groupby(['state', 'product_category'])['order_quantity'].sum().reset_index()\n",
    "most_popular_category_by_state = most_popular_category_by_state.sort_values(['state', 'order_quantity'], ascending=[True, False])\n",
    "most_popular_category_by_state = most_popular_category_by_state.drop_duplicates(subset=['state'])\n",
    "\n",
    "print(\"Most popular product category in each state:\")\n",
