   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Group the data by product category and calculate the total sales\n",
    "total_sales_by_category = sales_df.groupby('product_category')['order_amount_(â‚¹)'].sum()\n",
    "\n",
    "# Filter the categories to include specific ones\n",
    "categories_to_include = ['Set', 'kurta', 'Western Dress', 'Top', 'Ethnic Dress', 'Blouse']\n",
    "filtered_sales_by_category = total_sales_by_category.loc[categories_to_include]\n",
    "\n",
    "# Calculate the total revenue\n",
    "total_revenue = filtered_sales_by_category.sum()\n",
    "\n",
    "# Calculate the percentage of total revenue for each category\n",
    "percentage_sales_by_category = (filtered_sales_by_category / total_revenue) * 100\n",
    "\n",
    "# Sort the categories by total sales in descending order\n",
    "percentage_sales_by_category = percentage_sales_by_category.sort_values(ascending=False)\n",
    "\n",
    "# Create a bar chart to show the sales by product category\n",
    "fig, ax = plt.subplots(figsize=(12, 8))\n",
    "palette_colors = ['#F0AB00' if category in ['Set', 'Western Dress'] else '#878787' for category in percentage_sales_by_category.index]\n",
    "sns.barplot(x=percentage_sales_by_category.index, y=percentage_sales_by_category.values, ax=ax, palette=palette_colors)\n",
    "\n",
    "# Set y-axis tick values and limits\n",
    "num_y_ticks = 10\n",
    "y_tick_values = list(range(0, 101, 10))\n",
    "ax.set_yticks(y_tick_values)\n",
    "ax.set_ylim(0, 100)\n",
    "\n",
    "# Set labels and title\n",
    "ax.set_ylabel('Percentage of Total Revenue', labelpad=10)\n",
    "ax.set_xlabel('Product Category', labelpad=10)\n",
    "ax.set_title('Percentage of Total Revenue by Product Category', fontsize=20, pad=40)\n",
    "ax.text(0.28, 1.04, 'Average Cost per Product Displayed', fontsize=15, color='#878787', transform=ax.transAxes)\n",
    "\n",
    "# Format the y-axis ticks and add grid lines\n",
    "ax.yaxis.set_major_formatter(plt.FuncFormatter(lambda x, _: f'{int(x)}%'))\n",
    "ax.yaxis.grid(linestyle='--', color='gray', linewidth=0.5, dashes=(8, 5))\n",
    "ax.xaxis.grid(False)\n",
    "\n",
    "# Add annotations for average cost per product\n",
    "for i, category in enumerate(percentage_sales_by_category.index):\n",
    "    avg_cost = sales_df[sales_df['product_category'] == category]['order_amount_(â‚¹)'].mean()\n",
    "    fontsize = 18 if category in ['Set', 'Western Dress'] else 13\n",
    "    fontweight = 'bold' if category in ['Set', 'Western Dress'] else 'normal'\n",
    "    ax.text(i, percentage_sales_by_category.values[i] + 0.5, f'â‚¹{avg_cost:.2f}', ha='center', fontsize=fontsize, fontweight=fontweight)\n",
    "\n",
    "# Add a callout to emphasize the importance of Western Dresses\n",
    "western_sales_pct = percentage_sales_by_category['Western Dress']\n",
    "western_index = percentage_sales_by_category.index.get_loc('Western Dress')\n",
    "\n",
    "# Define the callout box properties\n",
    "bbox_props = dict(boxstyle='round,pad=0.3', facecolor='white', edgecolor='black', alpha=0.9)\n",
    "arrow_props = dict(facecolor='black', arrowstyle='fancy', alpha=0.7)\n",
    "\n",
    "# Set the position of the callout box\n",
    "x_pos = western_index\n",
    "y_pos = western_sales_pct + 3\n",
    "x_text = x_pos + 1\n",
    "y_text = y_pos + 30\n",
    "\n",
    "# Add the callout annotation\n",
    "callout_text = (\n",
    "    'With their high price point and strong sales performance,\\n'\n",
    "    'western dresses are a key driver of our revenue.\\n'\n",
    "    'By prioritizing the sale of these products,\\n'\n",
    "    'we can build a more resilient and diversified business.'\n",
    ")\n",
    "ax.annotate(callout_text, xy=(x_pos, y_pos), xytext=(x_text, y_text), fontsize=14,\n",
    "            arrowprops=arrow_props, bbox=bbox_props, ha='center', va='center')\n",
    "\n",
    "# Remove top and right spines, and style the bottom spine\n",
    "ax.spines['top'].set_visible(False)\n",
    "ax.spines['right'].set_visible(False)\n",
    "ax.spines['left'].set_visible(False)\n",
    "ax.spines['bottom'].set_linewidth(2)\n",
    "ax.spines['bottom'].set_color('black')\n",
    "\n",
    "# Show the plot\n",
    "plt.show()\n",
    "\n",
    "# Save the figure\n",
    "fig.savefig('Percent_Prod_Revenue.png', dpi=300)\n"
