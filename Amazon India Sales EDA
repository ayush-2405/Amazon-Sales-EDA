    "import warnings\n",
    "import numpy as np\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "# Ignore warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "# Set the style for the plot\n",
    "sns.set_style('whitegrid')\n",
    "plt.style.use('seaborn-dark-palette')\n",
    "\n",
    "# Define the desired order of months\n",
    "month_order = ['April', 'May', 'June']\n",
    "\n",
    "# Filter the data to only include the four product categories of interest\n",
    "selected_categories = ['Western Dress', 'Top', 'kurta', 'Set']\n",
    "sales_data = sales_df[sales_df['product_category'].isin(selected_categories)]\n",
    "\n",
    "# Convert the date column to a datetime object\n",
    "sales_data['date'] = pd.to_datetime(sales_data['date'])\n",
    "\n",
    "# Extract the month from the date column and set it as a new column\n",
    "sales_data['month'] = sales_data['date'].dt.month_name()\n",
    "\n",
    "# Aggregate the sales data by month and product category\n",
    "sales_by_month = sales_data.groupby(['month', 'product_category'])['order_amount_(â‚¹)'].sum().reset_index()\n",
    "\n",
    "# Convert the month column to a categorical data type with the desired order\n",
    "sales_by_month['month'] = pd.Categorical(sales_by_month['month'], categories=month_order, ordered=True)\n",
    "\n",
    "# Plot the sales data using seaborn\n",
    "fig, ax = plt.subplots(figsize=(10, 6))\n",
    "sns.barplot(x='month', y='order_amount_(â‚¹)', hue='product_category', data=sales_by_month,\n",
    "            palette=['#969696', '#73C5C5', '#F0AB00', '#B2B0EA'], ax=ax)\n",
    "\n",
    "# Extract the sales data for Western Dress\n",
    "sales_wd = sales_by_month[sales_by_month['product_category'] == 'Western Dress'].reset_index(drop=True)\n",
    "sales_wd['month'] = pd.Categorical(sales_wd['month'], categories=month_order, ordered=True)\n",
    "sales_wd.sort_values(by='month', inplace=True)\n",
    "\n",
    "# Add line plot for total monthly revenue of Western Dress\n",
    "ax.plot([0.1, 1.1, 2.1], sales_wd['order_amount_(â‚¹)'], color='black', linestyle='dashdot', linewidth=2, marker='o')\n",
    "\n",
    "# Add annotation for percent increase from April to June for Western Dress\n",
    "pct_increase = (sales_wd.loc[1, 'order_amount_(â‚¹)'] - sales_wd.loc[0, 'order_amount_(â‚¹)']) / sales_wd.loc[0, 'order_amount_(â‚¹)'] * 100\n",
    "ax.annotate(f'{pct_increase:.0f}% increase\\n April to June',fontweight='bold', xy=(2.1, sales_wd.loc[2, 'order_amount_(â‚¹)']), xytext=(2.88, sales_wd.loc[2, 'order_amount_(â‚¹)'] + 40000),\n",
    "            arrowprops=dict(arrowstyle='fancy', color='black', lw=1.5, connectionstyle=\"arc3,rad=0.1\"))\n",
    "\n",
    "# Set the number of y ticks you want\n",
    "num_y_ticks = 10\n",
    "\n",
    "# Calculate the y tick values\n",
    "y_tick_values = np.linspace(ax.get_yticks()[0], ax.get_yticks()[-1], num_y_ticks)\n",
    "\n",
    "# Set the y ticks\n",
    "ax.set_yticks(y_tick_values)\n",
    "\n",
    "# Add title and axis labels\n",
    "ax.set_title('Top Product Revenue by Month', fontsize=20, pad=20, color='#4c72b0')\n",
    "ax.text(-0.09, 1.04, 'Q2 FY22', fontsize=15, color='#878787', transform=ax.transAxes)\n",
    "ax.set_xlabel(None)\n",
    "ax.set_ylabel('Net Revenue in â‚¹10,000', fontsize=14, labelpad=10)\n",
    "\n",
    "# Format y-axis ticks\n",
    "ax.set_yticklabels([f'{int(x)}k' for x in range(0, 46, 5)])\n",
    "\n",
    "# Add gridlines\n",
    "ax.yaxis.grid(linestyle='--', color='gray', linewidth=0.7, dashes=(8, 5))\n",
    "\n",
    "# Add legend\n",
    "plt.legend(bbox_to_anchor=(1, 1), fontsize=12, framealpha=1)\n",
    "\n",
    "# Format tick parameters\n",
    "ax.tick_params(axis='both', labelsize=12)\n",
    "\n",
    "# Remove top and right spines, and adjust the bottom spine\n",
    "ax.spines['top'].set_visible(False)\n",
    "ax.spines['right'].set_visible(False)\n",
    "ax.spines['left'].set_visible(False)\n",
    "ax.spines['bottom'].set_linewidth(2)\n",
    "ax.spines['bottom'].set_color('black')\n",
    "\n",
    "# Show the plot\n",
    "plt.show()\n",
    "\n",
    "# Save the figure\n",
    "fig.savefig('Avg_MOV.png', dpi=300)\n",
    "\n",
    "# Re-enable warnings\n",
    "warnings.filterwarnings('default')\n"
