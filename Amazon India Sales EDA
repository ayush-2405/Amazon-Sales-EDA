    "import matplotlib.ticker as ticker\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Set the style for the plot\n",
    "sns.set_style('whitegrid')\n",
    "plt.style.use('seaborn-dark-palette')\n",
    "\n",
    "# Group the data by month and calculate the total sales revenue\n",
    "monthly_revenue = sales_df.groupby(pd.Grouper(key='date', freq='ME')).agg({'order_amount_(â‚¹)': 'sum'})\n",
    "\n",
    "# Get the revenue for the latest month and the average revenue for the last quarter\n",
    "latest_month_revenue = monthly_revenue.tail(1).iloc[0][0]\n",
    "average_quarterly_revenue = monthly_revenue.tail(3).head(2).mean()[0]\n",
    "\n",
    "# Compute percentage below the average quarterly revenue for the latest month\n",
    "percentage_below_average = round((1 - (latest_month_revenue / average_quarterly_revenue)) * 100, 1)\n",
    "\n",
    "# Plot the monthly sales revenue\n",
    "fig, ax = plt.subplots(figsize=(10, 6))\n",
    "bars = ax.bar(monthly_revenue.index.strftime('%b'), monthly_revenue['order_amount_(â‚¹)'], color='#4c72b0', edgecolor='black')\n",
    "\n",
    "# Add label above each bar with the percentage below the average revenue for the quarter\n",
    "for i, bar in enumerate(bars):\n",
    "    if i == len(bars) - 1 or i < len(bars) - 2:\n",
    "        continue\n",
    "    current_month_revenue = monthly_revenue.iloc[i]['order_amount_(â‚¹)']\n",
    "    percentage_below_avg = round((1 - (current_month_revenue / average_quarterly_revenue)) * 100, 1)\n",
    "    ax.annotate(f'{percentage_below_avg}% below avg.', \n",
    "                xy=(bar.get_x() + bar.get_width() / 2, bar.get_height() - 7000), \n",
    "                xytext=(0, 2), textcoords='offset points', fontweight='bold', \n",
    "                ha='center', va='bottom', fontsize=12, color='darkred')\n",
    "\n",
    "# Add label above the latest bar with the percentage below the average revenue for the quarter\n",
    "latest_bar = bars[-1]\n",
    "latest_month_sales = latest_bar.get_height()\n",
    "percentage_below_avg = round((1 - (latest_month_sales / average_quarterly_revenue)) * 100, 1)\n",
    "ax.annotate(f'{percentage_below_avg}% below avg.', \n",
    "            xy=(latest_bar.get_x() + latest_bar.get_width() / 2, latest_bar.get_height() - 7000), \n",
    "            xytext=(0, 5), textcoords='offset points', fontweight='bold',\n",
    "            ha='center', va='bottom', fontsize=12, color='darkred')\n",
    "\n",
    "# Add a horizontal line at the average quarterly revenue\n",
    "plt.axhline(average_quarterly_revenue, linestyle='dashdot', color='#F0AB00', linewidth=2, label='Q2 Average Revenue')\n",
    "\n",
    "# Set plot title and labels\n",
    "ax.set_title('Sales Revenue in India', fontsize=20, pad=20, color='#4c72b0')\n",
