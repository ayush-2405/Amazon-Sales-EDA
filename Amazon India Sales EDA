total percentage decrease in revenue from April to June\ntotal_percent_decrease_q2 = (monthly_revenue['april'] - monthly_revenue['june']) / monthly_revenue['april'] * 100\n\n# Print revenue details and percentage decrease for Q2\nprint(f\"Total revenue for April 2022: â‚¹{monthly_revenue['april']:,.2f}\")\nprint(f\"Total revenue for May 2022: â‚¹{monthly_revenue['may']:,.2f}, which is a {percent_decrease_apr_to_may:.2f}% decrease from April.\")\nprint(f\"Total revenue for June 2022: â‚¹{monthly_revenue['june']:,.2f}, which is a {percent_decrease_may_to_jun:.2f}% decrease from May.\")\nprint(f\"Total revenue for Q2 2022 decreased by {total_percent_decrease_q2:.2f}%\")\nprint(\"\\n\")\n\n# Calculate and print total revenue by product category\nrevenue_by_category = sales_df.groupby('product_category')['order_amount_(â‚¹)'].sum().sort_values(ascending=False)\nprint(\"Total revenue by product category:\")\nprint(revenue_by_category.apply(lambda x: f\"â‚¹{x:,.2f}\"))\nprint(\"\\n\")\n\n# Calculate and print percentage of revenue by product category\npercent_revenue_by_category = (revenue_by_category / revenue_by_category.sum() * 100).sort_values(ascending=False)\npercent_revenue_by_category = percent_revenue_by_category.apply(lambda x: f\"{x:.2f}%\")\nprint(\"Percentage of revenue by product category:\")\nprint(percent_revenue_by_category)\nprint(\"\\n\")\n\n# Calculate and print average order amount by product category\naverage_order_amount_by_category = sales_df.groupby('product_category')['order_amount_(â‚¹)'].mean().sort_values(ascending=False)\nprint(\"Top 5 product categories by average order amount:\")\nprint(average_order_amount_by_category.head(5))\nprint(\"\\n\")\n\n# Calculate and print statistics on cancelled and returned orders\ncancelled_orders = sales_df[sales_df['ship_status'].isin(['Cancelled', 'Shipped - Lost in Transit'])]\nreturned_orders = sales_df[sales_df['ship_status'].isin(['Shipped - Returned to Seller', 'Shipped - Returning to Seller', 'Shipped - Rejected by Buyer', 'Shipped - Damaged'])]\n\ntotal_cancelled_orders = len(cancelled_orders)\ntotal_returned_orders = len(returned_orders)\ntotal_cancelled_and_returned_orders = total_cancelled_orders + total_returned_orders\n\npercent_cancelled_orders = total_cancelled_orders / len(sales_df) * 100\npercent_returned_orders = total_returned_orders / len(sales_df) * 100\npercent_cancelled_and_returned_orders = total_cancelled_and_returned_orders / sales_df['order_quantity'].sum() * 100\n\nprint(f\"Total cancelled orders: {total_cancelled_orders}, which is {percent_cancelled_orders:.2f}% of all orders.\")\nprint(f\"Total returned orders: {total_returned_orders}, which is {percent_returned_orders:.2f}% of all orders.\")\nprint(f\"This represents {percent_cancelled_and_returned_orders:.2f}% of all orders.\")\nprint(\"\\n\")\n\n# Calculate and print average order amount and quantity by month\nmonthly_order_stats = sales_df.groupby(pd.Grouper(key='date', freq='ME')).agg({\n    'order_amount_(â‚¹)': 'mean', \n    'order_quantity': 'mean'\n}).rename(columns={'order_amount_(â‚¹)': 'average_order_amount', 'order_quantity': 'average_order_quantity'})\nprint(monthly_order_stats)\nprint(\"\\n\")\n\n# Calculate and print the most popular product category in each state\nmost_popular_category_by_state = sales_df.groupby(['state', 'product_category'])['order_quantity'].sum().reset_index()\nmost_popular_category_by_state = most_popular_category_by_state.sort_values(['state', 'order_quantity'], ascending=[True, False])\nmost_popular_category_by_state = most_popular_category_by_state.drop_duplicates(subset=['state'])\n\nprint(\"Most popular product category in each state:\")\nprint(most_popular_category_by_state)\nprint(\"\\n\")\n\n# Calculate and print average order amount by customer type\naverage_order_amount_by_customer_type = sales_df.groupby('customer_type')['order_amount_(â‚¹)'].mean()\nprint(\"Average order amount by customer type:\")\nprint(average_order_amount_by_customer_t