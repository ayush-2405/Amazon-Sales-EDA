    "import pandas as pd\n",
    "import numpy as np\n",
    "import seaborn as sns\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Group the data by product size and calculate the total sales\n",
    "total_sales_by_size = sales_df.groupby('size')['order_amount_(â‚¹)'].sum()\n",
    "\n",
    "# Create a horizontal bar chart to show the sales by product size\n",
    "fig, ax = plt.subplots(figsize=(12, 6))\n",
    "\n",
    "# Use a color palette to highlight specific sizes (S, M, L)\n",
    "highlight_sizes = ['S', 'M', 'L']\n",
    "palette_colors = ['#F0AB00' if size in highlight_sizes else '#878787' for size in total_sales_by_size.index]\n",
    "sns.barplot(x=total_sales_by_size.index, y=total_sales_by_size.values, ax=ax, palette=palette_colors)\n",
    "\n",
    "# Set labels and title\n",
    "ax.set_xlabel('Product Size', labelpad=10, fontsize=14)\n",
    "ax.set_ylabel('Net Revenue in â‚¹10,000', labelpad=10, fontsize=14)\n",
    "ax.set_title('Sales by Product Size', fontsize=20, pad=20)\n",
    "ax.text(-0.06, 1.04, 'Q2 FY22', fontsize=15, color='#878787', transform=ax.transAxes)\n",
    "\n",
    "# Format y-axis ticks and add grid lines\n",
    "num_y_ticks = 10\n",
    "y_tick_values = np.linspace(0, total_sales_by_size.max(), num_y_ticks)\n",
    "ax.set_yticks(y_tick_values)\n",
    "ax.yaxis.set_major_formatter(plt.FuncFormatter(lambda x, _: f'{int(x/10000)}k'))\n",
    "ax.yaxis.grid(linestyle='--', color='gray', linewidth=0.5, dashes=(8, 5))\n",
    "ax.xaxis.grid(False)\n",
    "\n",
    "# Add annotations for S, M, and L sizes\n",
    "for i, size in enumerate(total_sales_by_size.index):\n",
    "    if size in highlight_sizes:\n",
    "        ax.text(i, total_sales_by_size.values[i], f'{total_sales_by_size.values[i] / 10000:.0f}k', \n",
    "                ha='center', fontsize=14, fontweight='bold', color='black')\n",
