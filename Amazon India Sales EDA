ales = sales_df[sales_df['state'].isin(top_states)]\n\n# Group the data by state and product category, and calculate the total sales\nsales_by_state_product = filtered_sales.groupby(['state', 'product_category'], group_keys=True)['order_amount_(â‚¹)'].sum().reset_index()\n\n# Get the top 3 products for each state in each quartile\ntop_products = []\nfor quartile in ['Q1', 'Q2', 'Q3', 'Q4']:\n    top_products += sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == quartile].index)].groupby('state', group_keys=True).apply(lambda x: x.nlargest(3, 'order_amount_(â‚¹)'))['product_category'].tolist()\n\n# Create a figure with four subplots\nfig, axs = plt.subplots(2, 2, figsize=(16, 12), sharey=False)\n\n# Function to create a bar plot for each quartile\ndef create_barplot(quartile_sales, ax, title):\n    sns.barplot(x='state', y='order_amount_(â‚¹)', hue='product_category', data=quartile_sales, palette=colors, ax=ax)\n    ax.legend().set_visible(False)\n    ax.set_title(title)\n    ax.set_xlabel('State')\n    ax.set_ylabel('Total Sales (â‚¹)')\n\n# Plot sales for quartile 1\nq1_sales = sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == 'Q1'].index)]\nq1_sales = q1_sales[q1_sales['product_category'].isin(top_products)]\ncreate_barplot(q1_sales, axs[0, 0], 'Sales by Q1 States (lowest revenue quartile)')\n\n# Plot sales for quartile 2\nq2_sales = sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == 'Q2'].index)]\nq2_sales = q2_sales[q2_sales['product_category'].isin(top_products)]\ncreate_barplot(q2_sales, axs[0, 1], 'Sales by Q2 States (second-lowest revenue quartile)')\n\n# Plot sales for quartile 3\nq3_sales = sales_by_state_product[sales_by_state_product['state'].isin(sales_by_state[quartiles == 'Q3'].index)]\nq3_sales = q3_sales[q3_sales['product_category'].isin(top_products)]\ncreate_barplot(q3_sales, axs[1, 0], 'Sales by Q3 States (second-highest revenue quartil